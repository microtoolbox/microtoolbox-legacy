# Enable TLSv1.2 for compatibility with older clients
[System.Net.ServicePointManager]::SecurityProtocol = [System.Net.ServicePointManager]::SecurityProtocol -bor [System.Net.SecurityProtocolType]::Tls12

try {
    Start-BitsTransfer @("https://get-ms.github.io/batbox.exe","https://get-ms.github.io/getinput.exe") -Destination @("$env:temp\batbox.exe","$env:temp\getinput.exe") -Priority Foreground
} catch {
    Write-Error $_
	Return
}